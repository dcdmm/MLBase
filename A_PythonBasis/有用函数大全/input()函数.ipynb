    "# 打印倒三角形(非等腰)\n",
    "temp1 = int(input(\"请再输入一个数\"))\n",
    "start = temp1\n",
    "while start >= 1:\n",
    "    print(end=\" \" * start)\n",
    "    print(\"*\" * start)\n",
    "    start = start - 1"
