   "source": [
    "def hello(fn):\n",
    "\tdef wrapper():\n",
    "\t\tprint(\"hello, %s\" %fn.__name__)\n",
    "\t\tfn()\n",
    "\t\tprint(\"goodby, %s\"%fn.__name__)\n",
    "        \n",
    "\treturn wrapper\n",
    "\n",
    "@hello\n",
    "def foo(): # 等同于:hello(foo)\n",
    "\tprint(\"i am foo\")\n",
    "\n",
    "foo()\n",
    "print(foo.__name__) # 输出wrapper"
   ]
