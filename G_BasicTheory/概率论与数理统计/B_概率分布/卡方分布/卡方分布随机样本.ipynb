   "source": [
    "import numpy as np\n",
    "from scipy.stats import chi2\n",
    "\n",
    "# draw a single sample\n",
    "np.random.seed(42)\n",
    "print(chi2.rvs(df=4), end=\"\\n\\n\")\n",
    "\n",
    "# draw 10 samples\n",
    "print(chi2.rvs(df=4, size=10), end=\"\\n\\n\")\n"
   ],
