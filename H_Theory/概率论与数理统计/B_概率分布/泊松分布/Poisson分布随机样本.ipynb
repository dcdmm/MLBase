    "import numpy as np\n",
    "from scipy.stats import poisson\n",
    "\n",
    "# draw a single sample\n",
    "np.random.seed(42)\n",
    "print(poisson.rvs(mu=10), end=\"\\n\\n\")\n",
    "\n",
    "# draw 10 samples\n",
    "print(poisson.rvs(mu=10, size=10), end=\"\\n\\n\")"
