   "source": [
    "arr1 = torch.tensor([1, 2, 3, 4, 5])\n",
    "arr2 = torch.tensor([2, 3, 4, 5, 6])\n",
    "print(arr1 @ arr2)  # 向量的内积(点积)\n",
    "print(torch.dot(arr1, arr2))  # 与上等价"
   ]
  },
