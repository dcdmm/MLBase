    "import itertools\n",
    "\n",
    "print('单独的迭代器拼接')\n",
    "for i in itertools.chain('abc', [1, 2, 3]):\n",
    "    print(i)\n",
    "\n",
    "print('用iterable的元素拼接,iterable的元素必须为可迭代：')\n",
    "for i in itertools.chain.from_iterable(['abc', [1, 2, 3]]):\n",
    "    print(i)\n"
   ]
